<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}DRONE NATION{% endblock %}</title>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
        <meta http-equiv="x-ua-compatible" content="ie=edge">
        <link rel="icon" type="image/x-icon" href="{{ asset("favicon.ico") }}"/>
        {% block stylesheets %}
            <link href="{{ asset('build/layout.css') }}" rel="stylesheet">
            <link href="{{ asset('build/app_main.css') }}" rel="stylesheet">
        {% endblock %}
    </head>
    <body>
    <header>
        {% block header %}
            {% include 'app/blocks/sidenav.html.twig' %}
            {% include 'app/blocks/navigation.html.twig' %}
        {% endblock %}
    </header>
        <main>
            <div class="cointainer-fluid">
        {% block body %}

        {% endblock %}
            </div>
        </main>
        {% block javascripts %}
            <script type="text/javascript" src="{{ asset('build/manifest.js') }}"></script>
            <script type="text/javascript" src="{{ asset('build/layout.js') }}"></script>
            <script type="text/javascript" src="{{ asset('build/app_main.js') }}"></script>
        {% endblock %}
    </body>
</html>
